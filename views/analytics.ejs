<script src="/bootstrap.min.js"></script>
<script src="/toastr.js"></script>
<script src="/helper.js"></script>
<script src="/analytics.js"></script>

<script type="text/javascript">
    let routes = [];
    let error_crons = [];
    let crons_not_in_use = [];

    $(function () {
        // initialize tooltips
        $('[data-toggle="tooltip"]').tooltip();
        routes = JSON.parse('<%- routes %>');
        error_crons = JSON.parse('<%- error_crons %>');
        crons_not_in_use = JSON.parse('<%- crons_not_in_use %>');
        updateErrorCronsPieChart();
        updateCronsNotInUseChart();
    })
</script>
