        <div class="row">
            <div class="col-lg-6">
                <div class="panel panel-default">
                    <div class="panel-heading">Error Crons VS Success Crons</div>
                    <div class="panel-body">
                        <div id="highcharts_1"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="panel panel-default">
                    <div class="panel-heading">Crons In Use VS Not In Use</div>
                    <div class="panel-body">
                        <div id="highcharts_2"></div>
                    </div>
                </div>
            </div>
        </div>
